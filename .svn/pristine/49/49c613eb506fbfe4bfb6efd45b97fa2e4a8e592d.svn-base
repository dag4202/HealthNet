body {
    background: url(resources/background.png);
    margin: 0;
    min-width: 980px;
    font-family: sans-serif;
    color: #676767;
}
#content_left {
    position: relative;
    background: #FFFFFF;
    min-height: 600px;
    width: 960px;
    margin: 20px auto;
    border-radius: 30px;
    overflow: hidden;
    box-shadow: 0 0 5px #E8E8E8;
}
#regButton {
	position: relative;
	height: 200px;
	width: 1000px;
	bottom: 0px;
}
#header {
    position: relative;
    background-color: #91d5da;
    width: 960px;
    height: 120px;
    margin: auto;
    border-radius: 0 0 30px 30px;
    overflow: hidden;
    box-shadow: 0 0 5px #BBBBBB;
}
.right_header {
    display: inline-block;
    padding: 30px 0 10px 10px;
    font-size: 14px;
    font-weight: bold;
}
#logout {
    position: absolute;
    color: #FFF;
    margin: 10px 20px;
    top: 0;
    right: 0;
    cursor: pointer;
}
#calendar {
    margin: 15px 0;
    background: #FFFFFF;
    display: inline-block;
    height: 180px;
    width: 315px;
    top: 72px;
    border-top: 1px solid #E8E8E8;
    border-bottom: 1px solid #E8E8E8;
}
#logo {
    background: url(resources/logo.png);
    height: 32px;
    width: 189px;
    position: relative;
    top: 29px;
    left: 40px;
}
#logout {
    position: absolute;
    color: #FFF;
    margin: 10px 20px;
    top: 0;
    right: 0;
    cursor: pointer;
}

#logout:hover {
    text-decoration: underline;
}

#menu {
    position: absolute;
    background: #69beb4;
    height: 30px;
    width: 960px;
    bottom: 0px;
}

.menu {
    height: 30px;
    width: 480px;
    float: left;
    color: #FFF;
    padding-top: 5px;
    font-size: 20px;
    text-align: center;
}

.menu:hover {
    background: #91d5da;
}

